@model IEnumerable<ASPNetCoreAPI.Fit>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AccessoryId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BoatId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AccessoryId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BoatId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FitId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FitId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FitId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
